{"cells":[{"metadata":{"_uuid":"e59222f7e7bfce5d985f8304bc3b0f1347e5c9eb","trusted":true,"_cell_guid":"c6082455-2dd4-4843-b74a-66e500cf24b1"},"cell_type":"code","source":"library(caret)\nlibrary(e1071)\n\nreplace.na<-function(x){\n  if(is.na(x) || x=='NA'){\n    x=round(mean(t1$Age,na.rm=TRUE))\n  }\n  else{\n    x=x\n  }\n  return(x)\n}\n\nreplace.na1<-function(x){\n  if(is.na(x) || x=='NA'){\n    x=round(mean(t$Age,na.rm=TRUE))\n  }\n  else{\n    x=x\n  }\n  return(x)\n}\n\nt<-read.csv(\"../input/train.csv\",header=TRUE)\n\nt$Age<-sapply(t$Age,replace.na1)\nt$Age<-cut(t$Age,breaks = c(0,10,20,30,40,50,60,70,80))\nt$Survived=factor(t$Survived)\nt$Pclass=factor(t$Pclass)\nt$SibSp=factor(t$SibSp)\nt$Parch=factor(t$Parch,levels = c(0,1,2,3,4,5,6,9))\n\ncross<-createFolds(y=t$Survived,k=3)\n\n#validation data set\nvalid1=t[cross$Fold1,]\nvalid2=t[cross$Fold2,]\nvalid3=t[cross$Fold3,]\n\n#training data set\ntrain1=t[-cross$Fold1,]\ntrain2=t[-cross$Fold2,]\ntrain3=t[-cross$Fold3,]\n\n#Naive Bayes\nmod=naiveBayes(train1[,c(3,5:8)],train1[,2])\npredmod=predict(mod,newdata = valid1,type = \"class\")\na1=confusionMatrix(predmod,valid1$Survived)$overall[1]\n\nmod=naiveBayes(train2[,c(3,5:8)],train2[,2])\npredmod=predict(mod,newdata = valid2,type = \"class\")\na2=confusionMatrix(predmod,valid2$Survived)$overall[1]\n\nmod=naiveBayes(train3[,c(3,5:8)],train3[,2])\npredmod=predict(mod,newdata = valid3,type = \"class\")\na3=confusionMatrix(predmod,valid3$Survived)$overall[1]\n\nmean(c(a1,a2,a3))\n\nt=t[,c(1,3:12,2)]\n\nmod=naiveBayes(t[,c(2,4:7)],t[,12])\n\nt1<-read.csv(\"../input/test.csv\",header = TRUE)\n\nt1$Age<-sapply(t1$Age,replace.na)\nt1$Age<-cut(t1$Age,breaks = c(0,10,20,30,40,50,60,70,80))\nt1$Pclass=factor(t1$Pclass)\nt1$SibSp=factor(t1$SibSp)\nt1$Parch=factor(t1$Parch,levels = c(0,1,2,3,4,5,6,9))\n\npredmod=predict(mod,newdata = t1,type = \"class\")\n\nt1$Survived=predmod\n\nt1=t1[,c(1,12)]\n\nt1$Survived=c(ifelse(t1$Survived == 1 , 0 , 1))\n\nwrite.csv(t1,\"../output/submission.csv\",row.names = FALSE)\n","execution_count":null,"outputs":[]}],"metadata":{"kernelspec":{"display_name":"R","language":"R","name":"ir"},"language_info":{"mimetype":"text/x-r-source","name":"R","pygments_lexer":"r","version":"3.4.2","file_extension":".r","codemirror_mode":"r"}},"nbformat":4,"nbformat_minor":1}