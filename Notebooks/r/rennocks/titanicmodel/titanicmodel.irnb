df[-resampledataPart,]\n", "val_results <- predict(model, val)\n", "\n", "val_results"]}, {"execution_count": null, "metadata": {"_cell_guid": "82a229b7-84d6-4b83-a828-12ca8ddc60c3", "_uuid": "28b964281018354e708fb839f57370bc4020b96a"}, "cell_type": "code", "outputs": [], "source": ["cm <- confusionMatrix(val_results, val$Survived, positive = 'Yes')\n", "cm$byClass[['F1']]"]}, {"execution_count": null, "metadata": {"_cell_guid": "3bb6528e-f0e9-4b94-a7d2-7d6ad0fed7b3", "_uuid": "5bc7d0dc6a49afb7c85b0a80c4ecb0403e6cc0aa"}, "cell_type": "code", "outputs": [], "source": []}, {"execution_count": null, "metadata": {"_cell_guid": "bb83cf17-b79e-4a63-a8c0-673a83ac9da4", "_uuid": "0790f2445af6278583ca694e53d6acdc483e8575"}, "cell_type": "code", "outputs": [], "source": ["cm"]}, {"execution_count": null, "metadata": {"_cell_guid": "3d472b62-6a6e-427c-ad7c-828e805fd643", "_kg_hide-output": true, "_uuid": "f7fcdc14c6063dc63748b44202a8b3f87d91b9af"}, "cell_type": "code", "outputs": [], "source": ["vdf$Survived <- predict(model, vdf, na.action = na.pass)\n", "vdf$Survived <- ifelse(vdf$Survived == 'Yes', 1, 0)\n", "vdf$Survived"]}, {"execution_count": null, "metadata": {"_cell_guid": "05724e98-975b-40a6-9235-957b376ba2f9", "_uuid": "363d2084192013fa936d292c74fe2d3c184ed192"}, "cell_type": "code", "outputs": [], "source": ["write.csv(vdf[, c('PassengerId', 'Survived')], file = 'submission.csv', row.names = FALSE)\n", "Sys.sleep(1)"]}], "nbformat_minor": 1, "metadata": {"language_info": {"mimetype": "text/x-r-source", "codemirror_mode": "r", "pygments_lexer": "r", "name": "R", "file_extension": ".r", "version": "3.4.2"}, "kernelspec": {"language": "R", "display_name": "R", "name": "ir"}}, "nbformat": 4}