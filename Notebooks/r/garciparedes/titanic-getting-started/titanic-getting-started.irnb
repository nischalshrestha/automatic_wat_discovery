{"metadata": {"language_info": {"pygments_lexer": "r", "file_extension": ".r", "name": "R", "mimetype": "text/x-r-source", "version": "3.4.2", "codemirror_mode": "r"}, "kernelspec": {"name": "ir", "display_name": "R", "language": "R"}}, "nbformat": 4, "nbformat_minor": 1, "cells": [{"outputs": [], "cell_type": "code", "metadata": {"_uuid": "5d28fa3c027e0ea19905e39c57fc1001b2c362bf", "_cell_guid": "13b68f1b-04a6-4538-8bfe-b8a491993be7"}, "source": ["rm(list = ls())"], "execution_count": 29}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "f75f4671a659f47551cb5cc8b635cd7bc52e80c6", "_cell_guid": "b2db72d1-431d-441e-8f2b-eabd42407a3b"}, "source": ["input.dir <- './../input/'"], "execution_count": 30}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "68b1a1015ab38b8b21e9c484679c052b41c95597", "_cell_guid": "a4dd2105-46a1-494c-a7c3-b887340c6272"}, "source": ["library(MASS)"], "execution_count": 31}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "c8f0a116eba3f9fd471ab3189c180dbe013c7bbb", "_cell_guid": "d59382a0-f337-4782-b076-4d13e162b325"}, "source": ["removedColumnsIndex <- function(data, columIds) {\n", "    setdiff(colnames(data), columIds)\n", "}"], "execution_count": 32}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "6955d115480caf29cc2bcc375aa98571328e6eae", "_cell_guid": "3396f597-d851-45f0-8506-fc6087d38968"}, "source": ["columnsToNumeric <- function(data, columIds) {\n", "    for (i in columIds) {\n", "        data[,i] <- as.numeric(data[,i])\n", "    }\n", "    return(data)\n", "}"], "execution_count": 33}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "7bd95f5961aba58dbc7db1442c7c5738e61525e9", "_cell_guid": "1894b4f2-9893-4d96-ac91-b8c7fa3d4ee5"}, "source": ["regressionAge <- function(data) {\n", "    columns.to.remove <- c('Name', 'Cabin', 'Ticket', 'PassengerId')\n", "    data2 <- data[,removedColumnsIndex(data, columns.to.remove)]\n", "    \n", "    \n", "    data.train <- data2[!is.na(data2[,'Age']),]    \n", "    data.model <- lm( Age ~ . , data = data.train)\n", "    data.test <- data2[is.na(data2[,'Age']),]\n", "    \n", "    data[is.na(data[,'Age']),'Age'] <- predict(data.model, data.test)\n", "    return(data)\n", "}"], "execution_count": 34}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "730ce1c1a89f645c169859ec8be3127252cf786b", "_cell_guid": "b6e7b35f-f519-46ee-807c-082c7685e450"}, "source": ["preprocessData <- function(data) {\n", "    columns.to.remove <- c('Name', 'Cabin', 'Ticket')\n", "    data.cleaned <- data[,removedColumnsIndex(data, columns.to.remove)]\n", "\n", "    columns.to.numeric <- c('Sex', 'Embarked')\n", "    data.cleaned <- columnsToNumeric(data.cleaned, columns.to.numeric)\n", "    \n", "    columns.to.scale <- setdiff(colnames(data.cleaned), c('PassengerId', 'Survived'))\n", "    data.cleaned[,columns.to.scale] <- scale(data.cleaned[,columns.to.scale])\n", "    data.cleaned[is.na(data.cleaned)] <- 0\n", "    \n", "    \n", "    return(data.cleaned)\n", "}"], "execution_count": 35}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "c39731258bef64da9deb5382b3b760bbd2a4b934", "_cell_guid": "baf3f0ab-60de-4d69-93fd-f6a2ae498a2e"}, "source": ["predictData <- function(model, data) {\n", "    \n", "    data.prediction <- cbind(data[,'PassengerId'] , as.numeric(predict(model, data)$class) - 1)\n", "    colnames(data.prediction) <- c('PassengerId', 'Survived')\n", "\n", "    return(data.prediction)\n", "}"], "execution_count": 36}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "357806f81717d2c22d8ac3343a1874d0f929ffbe", "_cell_guid": "6918599c-cb21-4ee8-88a3-79df60b06ef0"}, "source": ["plotWithPCA <- function(data) {\n", "    data.pca <- princomp(data[,removedColumnsIndex(data, 'Survived')])\n", "    plot(data.pca$scores[,1:2], col = as.numeric(data[,'Survived']) + 2, pch = 20)\n", "}"], "execution_count": 37}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "26a02382fbb9f464e75bc2bb7fd025da83b00250", "_cell_guid": "b0a906d4-19ba-46e7-b78e-c26ce874d3e2"}, "source": ["plotWithLDA <- function(obj, data) {\n", "    plot(as.matrix(data[,removedColumnsIndex(data, 'Survived')]) %*% obj$scaling, \n", "         jitter(rep(0, dim(data)[1])), col = data[,'Survived'] + 2, \n", "         ylim = c(-0.05, 0.05), xlab = \"\", ylab = \"\", pch = 20)\n", "}"], "execution_count": 38}, {"outputs": [], "cell_type": "code", "metadata": {"_kg_hide-output": false, "_uuid": "f2e5f07442ba007040918ace20628fc2d715cded", "_cell_guid": "2cc6826d-ed75-4376-8beb-791765616172"}, "source": ["data.train <- read.csv(paste(input.dir, 'train.csv', sep =\"\"))\n", "data.test <- read.csv(paste(input.dir, 'test.csv', sep =\"\"))"], "execution_count": 39}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "30a1f4f49f74590a25f861c0d55a85517651e5ed", "_cell_guid": "66a5268b-ca88-4fea-bcee-97c12f1c035e"}, "source": ["summary(data.train)"], "execution_count": 40}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "55171688fe7d5c22b6beea10f4eba55228a77a30", "_cell_guid": "e59f40ae-a940-4c78-ab3f-5908273a006a"}, "source": ["head(data.train)"], "execution_count": 41}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "c8bf5d6e3f535b13fe57715ded0c65fcb7307d48", "_cell_guid": "f20fffed-7c1f-43ba-a8c4-688600f3ca05"}, "source": ["data.global <- regressionAge(rbind(data.train[,removedColumnsIndex(data, 'Survived')], data.test))\n", "data.train[,'Age'] <- data.global[1:dim(data.train)[1], 'Age']\n", "data.test[,'Age'] <- data.global[-c(1:dim(data.train)[1]), 'Age']"], "execution_count": 42}, {"outputs": [], "cell_type": "code", "metadata": {"scrolled": true, "_uuid": "7e42ef8df61209b2c2d6a13584c5def87de32da8", "_cell_guid": "3d4ea750-1db8-4344-b75e-e13e9156c476"}, "source": ["data.train.use <- preprocessData(data.train)\n", "head(data.train.use)"], "execution_count": 43}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "072d994ecb4ce37f647df32623b397b402e99118", "_cell_guid": "0a37c74d-eedd-424c-bedc-f16875f907e7"}, "source": ["pairs(data.train.use[, -c(1, 2)], col = as.numeric(data.train.use[, 2]) + 2, pch = 20)"], "execution_count": 44}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "98686fd3ac9eabc5c142958f116d6bc0c27ad293", "_cell_guid": "a8c2683a-02f1-40b9-ab49-4365174be80e"}, "source": ["plotWithPCA(data.train.use)"], "execution_count": 45}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "9f8bc82cb936653ce823c69422488293c3dc436a", "_cell_guid": "0dfca217-3d86-4347-9b57-8dce27f439dc"}, "source": ["data.test.use <- preprocessData(data.test)\n", "head(data.test.use)"], "execution_count": 46}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "3038ce2e39db9e85d8d129a5301997db6a55b2ac", "_cell_guid": "eaff714d-8cc5-47ff-8789-b793eb8889a3"}, "source": ["data.classifier.formula <- as.factor(Survived) ~ ."], "execution_count": 47}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "7cdee9d909ffe910b121a68047bb28bb05240ee1", "_cell_guid": "ea369875-61a6-4f47-a8e4-a2e6790378c2"}, "source": ["data.classifier.lda <- lda(data.classifier.formula, data.train.use, prior = c(0.7, 0.3))\n", "\n", "table(as.numeric(predict(data.classifier.lda, data.train.use)$class) - 1, data.train.use[,2])\n", "\n", "mean(as.numeric(predict(data.classifier.lda, data.train.use)$class) - 1 == data.train.use[,2])"], "execution_count": null}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "0ef73f1ab23aec8423a51ac910a7f7c8c7c159d9", "_cell_guid": "1860301d-8de7-4066-828c-30ae9518888b"}, "source": ["plotWithLDA(data.classifier.lda, data.train.use)"], "execution_count": null}, {"outputs": [], "cell_type": "code", "metadata": {"_uuid": "6a637f4237f40872517b5615db94c7b1b60f7767", "_cell_guid": "005f61f1-4ee9-41ae-a5c7-cf133865dd8f"}, "source": ["data.test.prediction.lda <- predictData(data.classifier.lda, data.test.use)\n", "\n", "head(data.test.prediction.lda)\n", "\n", "write.csv(data.test.prediction.lda,