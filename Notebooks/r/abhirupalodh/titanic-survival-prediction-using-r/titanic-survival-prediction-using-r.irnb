["#Removing Outliers\n", "age_mean=mean(train_refined$Age,na.rm=TRUE)\n", "train_refined$Age[is.na(train_refined$Age)]=age_mean\n", "summary(train_refined)"]}, {"metadata": {"_cell_guid": "c1a14fa8-3ed8-4b2c-8f50-d4a2c691b58b", "collapsed": true, "_uuid": "e28ab610c09e0ddc00f9128b1d62aaabf2561e5f"}, "outputs": [], "cell_type": "code", "execution_count": null, "source": ["#Prediction \n", "library(randomForest)\n", "colnames(train_refined)\n", "train_refined=train_refined[,c(1,2,3,5,6,7,8,10)]"]}, {"metadata": {"_cell_guid": "17666df6-2e20-4379-8de8-01c84f38ab9f", "collapsed": true, "_uuid": "e2e5b09770251ccc7017912a279c098a748f9bf8"}, "outputs": [], "cell_type": "code", "execution_count": null, "source": ["nrow(train_refined)\n", "train_data=train_refined[1:625,]\n", "test_data=train_refined[626:891,]\n", "target=test_data[,2]\n", "as.factor(target)->target\n", "test_data=test_data[,-c(2)]\n", "test_data\n"]}, {"metadata": {"_cell_guid": "09db6e24-637e-4c4b-b311-4a1f5fe3c316", "collapsed": true, "_uuid": "eb5c9a8bb2bb292fd8262f4cc60d76e4fd120b9c", "scrolled": true}, "outputs": [], "cell_type": "code", "execution_count": null, "source": ["class(train_data$Survived)\n", "as.factor(train_data$Survived)->train_data$Survived"]}, {"metadata": {"_cell_guid": "b7d8ac1a-75c3-4619-b4b2-332e5586cad5", "collapsed": true, "_uuid": "ed96d6732bf7a339550be5bb38ea7c9568d6942d"}, "outputs": [], "cell_type": "code", "execution_count": null, "source": ["#'PassengerId' 'Pclass'  'Sex' 'Age' 'SibSp' 'Parch' 'Fare'\n", "\n", "model_randomForest=randomForest(Survived ~ PassengerId + Pclass + Sex + Age +  SibSp + Parch + Fare, data = train_data)\n", "model_randomForest\n"]}, {"metadata": {"_cell_guid": "0f0fa70e-0e01-4bd6-8c9e-3f4e07c3baa3", "collapsed": true, "_uuid": "27c484caef78a34b2f7f845e6cab8ac494911252"}, "outputs": [], "cell_type": "code", "execution_count": null, "source": ["pred = predict(model_randomForest,test_data)\n", "print(pred)\n", "data.frame(target,pred)->accuracy_tab"]}